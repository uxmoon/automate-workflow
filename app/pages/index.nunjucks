<!-- template -->
{% extends "layout.nunjucks" %}

<!-- Partial -->
{% block content %}
  <h1>This is the Homepage</h1>
  {% import "macros/nav-macro.nunjucks" as nav %}
  {{nav.active('home')}}

  <div class="gallery">
    <!-- Loops through images array -->
    {% for image in images %}
    <div class="gallery__item">
      <img src="{{image.src}}" alt="{{image.alt}}">
    </div>
    {% endfor %}
  </div>
{% endblock %}
